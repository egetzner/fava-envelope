; Do not open accounts automatically, otherwise use 'open_date': '1988-02-22',
plugin "beancount_share.share" "{
    'mark_name': 'share',
    'meta_name': 'shared',
    'account_debtors': 'Liabilities:Debtors',
    'account_creditors': 'Assets:Receivables:Creditors',
    'open_date': '1988-02-22',
    'quantize': '0.005'
}"

; ; this actually makes the ledger not balance out anymore if we remove this. split + spread just move it to different dates.
plugin "beancount_interpolate.recur" "{
    'aliases_after': ['recurAfter', 'recur'],
    'default_duration': '360',
    'default_step': '30',
    'default_method': 'SL',
    'min_value': 0.05,
    'max_new_tx': 9999,
    'suffix': ' (recur %d/%d)',
    'tag': 'recurred'
}"

;this slows down the journal/editor significantly, espcially if it's on a day to day basis.
;also it messes with the rent amounts, possibly some money is lost or shifted to a different period.
plugin "beancount_interpolate.split" "{
    'aliases_after': ['splitAfter', 'split'],
    'default_duration': 'Year',
    'default_step': 'Month',
    'default_method': 'SL',
    'min_value': 0.05,
    'max_new_tx': 9999,
    'suffix': ' (split %d/%d)',
    'tag': 'splitted'
}"


